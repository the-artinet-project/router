{
  "name": "@artinet/router",
  "version": "0.0.1",
  "description": "A tool for routing messages to the appropriate agents",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "rootDir": ".",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepare": "ts-patch install -s",
    "build": "tsc",
    "build:obfuscated": "./scripts/obfuscate.sh",
    "build:bundle": "npm run build && esbuild src/index.ts --bundle --platform=node --format=esm --minify=true --outfile=bundle/router-base.js --external:@artinet/sdk --external:@modelcontextprotocol/*",
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run clean && npm run build",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage"
  },
  "keywords": [
    "artinet",
    "router",
    "agent",
    "a2a",
    "agent2agent"
  ],
  "author": "artinet",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/the-artinet-project/artinet-sdk"
  },
  "dependencies": {
    "@artinet/sdk": "^0.5.7",
    "@artinet/types": "git+https://github.com/the-artinet-project/types.git#main",
    "@modelcontextprotocol/sdk": "^1.17.5",
    "p-limit": "^7.1.1",
    "uuid": "^11.1.0",
    "zod": "latest"
  },
  "devDependencies": {
    "@modelcontextprotocol/server-everything": "^2025.8.18",
    "@types/node": "^20.17.50",
    "@types/uuid": "^9.0.6",
    "esbuild": "^0.25.10",
    "eslint": "^9.25.1",
    "javascript-obfuscator": "^4.1.1",
    "jest": "^29.7.0",
    "msw": "^2.7.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.31.0",
    "typescript-transform-paths": "^3.5.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
