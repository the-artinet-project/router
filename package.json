{
  "version": "0.0.20-alpha.1",
  "name": "orc8",
  "description": "A dynamic orchestration library for A2A enabled AI agents that can call MCP servers.",
  "author": "artinet",
  "homepage": "https://artinet.io",
  "license": "Apache-2.0",
  "keywords": [
    "artinet",
    "orc8",
    "agent",
    "a2a",
    "agent2agent",
    "ai",
    "artificial intelligence",
    "mcp",
    "modelcontextprotocol",
    "orchestrator"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-artinet-project/orc8.git"
  },
  "issues": {
    "url": "https://github.com/the-artinet-project/orc8/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "rootDir": ".",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./openai": {
      "import": "./dist/api/openai.js",
      "types": "./dist/api/openai.d.ts",
      "require": "./dist/api/openai.js",
      "default": "./dist/api/openai.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:bundle": "npm run build && esbuild src/index.ts --bundle --platform=node --format=esm --minify=true --outfile=bundle/router-base.js --external:@artinet/sdk --external:@modelcontextprotocol/*",
    "clean": "rimraf dist",
    "clean:all": "rimraf dist node_modules/ package-lock.json",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run clean:all && npm i && npm run build && npm test",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage"
  },
  "dependencies": {
    "@artinet/sdk": "^0.6.0-preview.1",
    "@artinet/types": "^0.0.9-alpha.1",
    "p-limit": "^7.1.1",
    "uuid": "^11.1.0",
    "zod": "^3.25"
  },
  "peerDependencies": {
    "@modelcontextprotocol/sdk": "^1.24.3",
    "openai": "^6.15.0"
  },
  "devDependencies": {
    "@cfworker/json-schema": "^4.1.1",
    "@modelcontextprotocol/server-everything": "^2025.9.25",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.2",
    "@types/uuid": "^9.0.6",
    "esbuild": "^0.25.10",
    "eslint": "^9.25.1",
    "jest": "^29.7.0",
    "@modelcontextprotocol/sdk": "^1.24.3",
    "msw": "^2.7.5",
    "openai": "^6.15.0",
    "rimraf": "^5.0.5",
    "ts-jest": "29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.31.0"
  },
  "engines": {
    "node": ">=18.9.1"
  }
}
