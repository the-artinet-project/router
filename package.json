{
  "version": "0.0.5",
  "name": "@artinet/router",
  "description": "A tool for routing messages between A2A enabled AI agents that can call MCP servers.",
  "author": "artinet",
  "homepage": "https://artinet.io",
  "license": "Apache-2.0",
  "keywords": [
    "artinet",
    "router",
    "agent",
    "a2a",
    "agent2agent",
    "ai",
    "agentic",
    "ai agent",
    "artificial intelligence",
    "agentic ai"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/the-artinet-project/router"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "rootDir": ".",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:bundle": "npm run build && esbuild src/index.ts --bundle --platform=node --format=esm --minify=true --outfile=bundle/router-base.js --external:@artinet/sdk --external:@modelcontextprotocol/*",
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run clean && npm run build",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage"
  },
  "dependencies": {
    "@artinet/sdk": "^0.5.7",
    "@artinet/types": "0.0.4",
    "@modelcontextprotocol/sdk": "^1.19.1",
    "p-limit": "^7.1.1",
    "uuid": "^11.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@modelcontextprotocol/server-everything": "^2025.8.18",
    "@types/node": "^20.17.50",
    "@types/uuid": "^9.0.6",
    "esbuild": "^0.25.10",
    "eslint": "^9.25.1",
    "javascript-obfuscator": "^4.1.1",
    "jest": "^29.7.0",
    "msw": "^2.7.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.31.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
